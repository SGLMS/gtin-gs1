{
    "name": "sglms/gs1-gtin",
    "description": "Classes to create or validate GTIN (Global Trade Item Number)",
    "type": "library",
    "keywords": [
        "gs1",
        "gtin",
        "laravel",
        "sglms"
    ],
    "homepage": "https://sglms.com",
    "license": "MIT",
    "authors": [
        {
            "name": "James",
            "email": "james@sglms.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "picqer/php-barcode-generator": "^3.2"
    },
    "require-dev": {
        "laravel/framework": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Sglms\\Gs1Gtin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sglms\\Gs1Gtin\\Gs1ServiceProvider"
            ],
            "aliases": {
                "Gtin": "Sglms\\Gs1Gtin\\Facades\\Gtin",
                "Gs1": "Sglms\\Gs1Gtin\\Facades\\Gs1"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
